{
  "name": "@daniela-nx-sandbox/core",
  "private": true,
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "prepare": "./node_modules/.bin/husky install .config/husky",
    "lint": "nx run-many --target=lint . --output-style=stream",
    "clean": "nx run-many --target=clean",
    "build": "nx run-many --target=build",
    "typecheck": "nx run-many --target=typecheck --output-style=stream",
    "test:unit:staged": "nx run-many --target=test:unit --bail --findRelatedTests --colors --passWithNoTests",
    "test:unit": "nx run-many --target=test",
    "test:unit:coverage": "nx run-many --target=test:unit --coverage",
    "codegen": "graphql-codegen --config graphQl-types-generation.ts"
  },
  "engines": {
    "node": ">= 16.0.0",
    "npm": ">= 8.0.0",
    "yarn": "NOT SUPPORTED -- please use npm"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx,vue}": [
      "npx nx run-many --target=lint --fix",
      "npx nx run-many --target=test"
    ]
  },
  "devDependencies": {
    "@apollo/server": "4.7.5",
